import java.lang.*;

public class goofi{

             public static double[][]  exp(double x[][]){
                                      double[][] k=new double[x.length][x[0].length];
                                      for(int i=0;i<x.length;i++){
                                          for(int j=0;j<x[i].length;j++){
                                              k[i][j]=(double)Math.exp(x[i][j]);
                                                                        }
                                                                 }
                                      return k;
                                                      }
             public static double[][]  randomize(int x,int y){ 
                                      double[][] grid =new double[x][y];
                                      for(int i=0;i<grid.length;i++){
                                         for(int j=0;j<grid[i].length;j++){
                                             double randouble=(Math.random()*2)-1;
                                             System.out.println(randouble);
                                             grid[i][j]=randouble;
                                                                          }
                                                                    }
                                      return grid;
                                                            }
             public static double[][] transpose(double[][] x){
                                     
                                     double[][] k=new double[x[0].length][x.length];
                                     for(int i=0;i<k.length;i++){ 
                                          for(int j=0;j<k[i].length;j++){
                                              k[i][j]=x[j][i];
                                                                        }
                                                                }
                                     return k;
                                                           }
             public static double[][] matmul(double[][] x,double[][] y){
                                     double sum=0;
                                     
                                     double[][] mult=new double[x.length][y[0].length];
                                     for(int i=0;i<x.length;i++){
                                         for(int j=0;j<y[0].length;j++){
                                             for(int k=0;k<y.length;k++){
                                                 sum=sum+x[i][k]*y[k][j];
                                                                        }
                                             mult[i][j]=sum;
                                             sum=0;
                                                                       }
                                                                }
                                     return mult;
                                                                  }

             public static double[][]  abs(double x[][]){
                                      double[][] k=new double[x.length][x[0].length];
                                      for(int i=0;i<x.length;i++){
                                          for(int j=0;j<x[i].length;j++){
                                              k[i][j]=Math.abs(x[i][j]);
                                                                        }
                                                                 }
                                      return k;
                                                      }
             public static void printmat(double[][] x){
                                System.out.println();
                                System.out.print("[");
                                for(int i=0;i<x.length;i++){
                                    System.out.print("[");
