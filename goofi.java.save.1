                                        System.out.print(x[i][j]+" ");
                                                                  }
                                    System.out.print("]");
                                    System.out.println();
                                                           }
                                System.out.print("]");
                                System.out.println();
                                                     }

               public static double[][]  zeros(int x,int y){ 
                                      double[][] grid =new double[x][y];
                                      for(int i=0;i<grid.length;i++){
                                         for(int j=0;j<grid[i].length;j++){
                                             grid[i][j]=0;
                                                                          }
                                                                    }
                                      return grid;
                                                            }
               public static double[][] addmat(double[][] x,double[][] y){
                                       double[][]  sum=new double[x.length][y[0].length];
                                       for(int i=0;i<sum.length;i++){
                                           for(int j=0;j<sum[0].length;j++){
                                               sum[i][j]=x[i][j]+y[i][j];
                                                                           }
                                                                    }
                                       return sum;
                                                                      } 
               public static double[][] submat(double[][] x,double[][] y){
                                       double[][]  sub=new double[x.length][y[0].length];
                                       for(int i=0;i<sub.length;i++){
                                           for(int j=0;j<sub[0].length;j++){
                                               sub[i][j]=x[i][j]-y[i][j];
                                                                           }
                                                                    }
                                       return sub;
                                                                      } 
               public static double[][]  neg(double x[][]){
                                      double[][] k=new double[x.length][x[0].length];
                                      for(int i=0;i<x.length;i++){
                                          for(int j=0;j<x[i].length;j++){
                                              k[i][j]= -x[i][j];
                                                                        }
                                                                 }
                                      return k;
                                                        }
               public static double[][] subvalmat(int x,double[][] y){
                                       double[][]  sum=new double[y.length][y[0].length];
                                       for(int i=0;i<sum.length;i++){
                                           for(int j=0;j<sum[0].length;j++){
                                               sum[i][j]=x-y[i][j];
                                                                           }
                                                                    }
                                       return sum;
                                                                   }

              public static double[][] addvalmat(int x,double[][] y){
                                       double[][]  sum=new double[y.length][y[0].length];
                                       for(int i=0;i<sum.length;i++){
                                           for(int j=0;j<sum[0].length;j++){
                                               sum[i][j]=x+y[i][j];
                                                                           }
                                                                    }
                                       return sum;
                                                                      } 
              public static double[][] divmat(double x,double[][] y){
                                       double[][]  sum=new double[y.length][y[0].length];
                                       for(int i=0;i<sum.length;i++){
                                           for(int j=0;j<sum[0].length;j++){
                                               sum[i][j]=x/y[i][j];
                                                                           }
                                                                    }
                                       return sum;
                                                                   }
              public static double[][] dot(double[][] x,double[][] y){
                                       double[][]  sum=new double[x.length][y[0].length];
                                       for(int i=0;i<sum.length;i++){
                                           for(int j=0;j<sum[0].length;j++){
                                               sum[i][j]=x[i][j]*y[i][j];
                                                                           }
                                                                    }
                                       return sum;
                                                                      } 
              public static double mean(double x[][]){
                                      double k=0;
                                      for(int i=0;i<x.length;i++){
                                          for(int j=0;j<x[i].length;j++){
                                              k+=x[i][j];
                                                                        }
                                                                 }
                                      return k/x[0].length;
                                                      }
}
