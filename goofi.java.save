package numja;
import java.lang.*;

public class goofi{

             public static float[][]  exp(float x[][]){
                                      float[][] k=new float[x.length][x[0].length];
                                      for(int i=0;i<x.length;i++){
                                          for(int j=0;j<x[i].length;j++){
                                              k[i][j]=Math.getExponent(x[i][j]);
                                                                        }
                                                                 }
                                      return k;
                                                      }
             public static float[][]  randomize(int x,int y){ 
                                      float[][] grid =new float[x][y];
                                      for(int i=0;i<grid.length;i++){
                                         for(int j=0;j<grid[i].length;j++){
                                             grid[i][j]=(float)(Math.random()*2);
                                                                          }
                                                                    }
                                      return grid;
                                                            }
             public static float[][] transpose(float[][] x){
                                     
                                     float[][] k=new float[x[0].length][x.length];
                                     for(int i=0;i<k.length;i++){ 
                                          for(int j=0;j<k[i].length;j++){
                                              k[i][j]=x[j][i];
                                                                        }
                                                                }
                                     return k;
                                                           }
             public static float[][] matmul(float[][] x,float[][] y){
                                     float sum=0;
                                     
                                     float[][] mult=new float[x.length][y[0].length];
                                     for(int i=0;i<x.length;i++){
                                         for(int j=0;j<y[0].length;j++){
                                             for(int k=0;k<y.length;k++){
                                                 sum=sum+x[i][k]*y[k][j];
                                                                        }
                                             mult[i][j]=sum;
                                             sum=0;
                                                                       }
                                                                }
                                     return mult;
                                                                  }

             public static float[][]  abs(float x[][]){
                                      float[][] k=new float[x.length][x[0].length];
                                      for(int i=0;i<x.length;i++){
                                          for(int j=0;j<x[i].length;j++){
                                              k[i][j]=Math.abs(x[i][j]);
                                                                        }
                                                                 }
                                      return k;
                                                      }
             public static void printmat(float[][] x){
                                System.out.println();
                                System.out.print("[");
                                for(int i=0;i<x.length;i++){
                                    System.out.print("[");
                                    for(int j=0;j<x[i].length;j++){
                                        System.out.print(x[i][j]+" ");
                                                                  }
                                    System.out.print("]");
                                    System.out.println();
                                                           }
                                System.out.print("]");
                                System.out.println();
                                                     }

               public static float[][]  zeros(int x,int y){ 
                                      float[][] grid =new float[x][y];
                                      for(int i=0;i<grid.length;i++){
                                         for(int j=0;j<grid[i].length;j++){
                                             grid[i][j]=0;
                                                                          }
                                                                    }
                                      return grid;
                                                            }
               public static float[][] addmut(float[][] x,float[][] y){
                                       float[][]  sum=new float[x.length][y[0].length];
                                       for(int i=0;i<sum.length;i++){
                                           for(int j=0;j<sum[0].length;j++){
                                               sum[i][j]=x[i][j]+y[i][j];
                                                                           }
                                                                    }
                                       return sum;
                                                                      } 
               public static float[][] submut(float[][] x,float[][] y){
                                       float[][]  sub=new float[x.length][y[0].length];
                                       for(int i=0;i<sub.length;i++){
                                           for(int j=0;j<sub[0].length;j++){
                                               sub[i][j]=x[i][j]-y[i][j];
                                                                           }
                                                                    }
                                       return sub;
                                                                      } 
               public static float[][]  neg(float x[][]){
                                      float[][] k=new float[x.length][x[0].length];
                                      for(int i=0;i<x.length;i++){
                                          for(int j=0;j<x[i].length;j++){
                                              k[i][j]= -x[i][j];
                                                                        }
                                                                 }
                                      return k;
                                                        }
               public static float[][] submut(float x,float[][] y){
                                       float[][]  sum=new float[x.length][y[0].length];
                                       for(int i=0;i<sum.length;i++){
                                           for(int j=0;j<sum[0].length;j++){
                                               sum[i][j]=x-y[i][j];
                                                                           }
                                                                    }
                                       return sum;
                                                                   }

              }
